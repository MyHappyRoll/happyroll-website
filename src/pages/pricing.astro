<style>
  .hero-section {
    padding-top: 6rem;
  }

  #pricing {
    min-height: 480px;
  }

  .pricing-cards {
    gap: 20px;
  }

  .pricing-card {
    box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px;
    border-radius: 20px;
    max-width: 320px;
  }

  .pricing-card--top {
    height: 283.48px;
  }

  .feature-list {
    gap: 16px;
  }

  .feature-list li {
    gap: 10px;
  }

  .pricing-table__features-headings {
    border-radius: 30px;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px;
  }

  .pricing-table__feature:nth-child(odd) {
    background-color: var(--neutral-100);
  }

  .pricing-table__comparison-table {
    display: none;
  }

  .pricing-table__plans {
    width: 23.33%;
    height: 1340px;
    box-shadow: rgba(0, 0, 0, 0.05) 1px 1px 1px 0px;
  }

  .pricing-table__plans--left {
    border-top-left-radius: 30px;
  }

  .pricing-table__plans--right {
    border-top-right-radius: 30px;
  }

  .pricing-table__plans--last {
    border-bottom-right-radius: 30px;
  }

  .pricing-table__spacer {
    height: 20px;
  }

  .bi-check-lg {
    position: relative;
    top: -9px;
  }

  .h-72 {
    height: 72px;
  }

  .hidden {
    visibility: hidden;
  }

  .tooltip-container__tooltip {
    display: none;
    box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px;
  }

  .tooltip-container:hover {
    .tooltip-container__tooltip {
      display: block;
    }
  }

  @media screen and (min-width: 30rem) {
    .pricing-card {
      border-radius: 30px;
    }
  }

  @media screen and (min-width: 60rem) {
    .hero-section {
      padding-top: 4rem;
    }

    .pricing-table__comparison-table {
      display: flex;
    }

    .db-1024 {
      display: block;
    }
  }

  @media screen and (min-width: 66rem) {
    .pricing-section__container {
      padding-left: 0;
      padding-right: 0;
    }
  }
</style>

