<style>
  #pricing {
    min-height: 480px;
  }

  .pricing-cards {
    gap: 20px;
    /* When emphasizing Maki Master package as best value. */
    /* gap: 40px; */
  }

  .pricing-card {
    box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px;
    border-radius: 20px;
    max-width: 320px;
  }

  .pricing-card:nth-child(2) {
    /* To emphasize this package as best value. */
    /* transform: scale(1.15); */
  }

  .pricing-card--top {
    height: 283.48px;
  }

  .feature-list {
    gap: 16px;
  }

  .feature-list li {
    gap: 10px;
  }

  .pricing-table__features-headings {
    border-radius: 30px;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px;
  }

  .pricing-table__features {
    /* gap: 32px; */
  }

  .pricing-table__feature:hover {
    background-color: var(--neutral-100);
    transition: 250ms;
    i {
      opacity: 0.6;
      transition: 250ms;
    }
  }

  .pricing-table__comparison-table {
    display: none;
  }

  .pricing-table__plans {
    width: 23.33%;
    height: 1340px;
    box-shadow: rgba(0, 0, 0, 0.05) 1px 1px 1px 0px;
  }

  .pricing-table__plans--left {
    border-top-left-radius: 30px;
  }

  .pricing-table__plans--right {
    border-top-right-radius: 30px;
  }

  .pricing-table__plans--last {
    border-bottom-right-radius: 30px;
  }

  .pricing-table__spacer {
    height: 20px;
  }

  .bi-check-lg {
    position: relative;
    top: -9px;
  }

  .h-72 {
    height: 72px;
  }

  .hidden {
    visibility: hidden;
  }

  @media screen and (min-width: 480px) {
    .pricing-card {
      border-radius: 30px;
    }
  }

  @media screen and (min-width: 1024px) {
    .pricing-table__comparison-table {
      display: flex;
    }

    .db-1024 {
      display: block;
    }
  }

  @media screen and (min-width: 1060px) {
    .pricing-section__container {
      padding-left: 0;
      padding-right: 0;
    }
  }
</style>

